{
  "meta": {
    "name": "Beam topology â€” natural frequency maximization",
    "notes": "Passive flanges counted in volume fraction.",
    "units": {
      "length": "m",
      "force": "N",
      "density": "kg/m3",
      "elastic_modulus": "Pa"
    }
  },

  "domain": {
    "shape": "rectangular",
    "size": {
      "length": 8.0,
      "height": 1.0
    },
    "thickness": 1.0,
    "mesh": {
      "nelx": 400,
      "nely": 50
    },
    

    "load_cases": [
      {
        "name": "case1",
        "factor": 1.0,
        "loads": [
          { "type": "semi_harmonic", "mode": 1, "factor": 1.0 }
        ]
      }, 
        {
        "name": "case2",
        "factor": 0.0,
        "loads": [
          { "type": "semi_harmonic", "mode": 2, "factor": 1.0 }
        ]
      }
    ]
  },


  "material": {
    "model": "plane_stress_isotropic",
    "E": 1.0E7,
    "nu": 0.3,
    "rho": 1.0
  },
  
  "void_material": {
      "E_min_ratio": 1e-6,
      "rho_min": 1e-6
    },


  "bc": {
    "supports": [
		{ "type": "closest_point", "location": [0.0, 0.5], "dofs": ["ux", "uy"] },
        { "type": "closest_point", "location": [8.0, 0.5], "dofs": ["ux", "uy"] }
    ]
  },

  "optimisation": {
    "approach": "ourApproach",
    "optimizer": "MMA",
    "objective": "compliance",
    "interpolation": "SIMP",
    "volume_fraction": 0.3,
    "penalization": 3.0,

     "debug_semi_harmonic": true,
     "semi_harmonic_baseline": "solid",
     "semi_harmonic_rho_source": "x",

    "filter": {
      "type": "sensitivity",
      "radius": 2.0,
      "radius_units": "element",
      "boundary_condition": "symmetric"
    },

    "move_limit": 0.2,
    "max_iters": 2000,
    "convergence_tol": 4e-3,
    "seed": null,
    "visualise_live" : "yes",
    "visualise_quality" : "smooth",
    "visualise_modes" : 5,
    "save_snapshot_image" : "no",
    "save_final_image" : "yes",
    "save_frq_iterations" : "yes",
    "harmonic_normalize": true
  }
}
