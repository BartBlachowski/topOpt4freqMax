@article{Gomez2019,
title = {Topology optimization framework for structures subjected to stationary stochastic dynamic loads},
journal = {Structural and Multidisciplinary Optimization},
volume = {59},
pages = {813-833},
year = {2019},
doi = {10.1007/s00158-018-2103-3},
author = {Fernando Gomez and Billie F. Spencer Jr},
abstract = {The field of topology optimization has progressed substantially in recent years... Nevertheless, topology optimization of stochastically excited structures has received relatively little attention. In this study, a direct approach to problem is pursued, where the excitation is modeled as a stationary zero-mean filtered white noise. The stationary covariances of the structural responses of interest are obtained by solving a Lyapunov equation.}
