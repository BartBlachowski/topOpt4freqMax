doi = {10.1007/s00158-010-0595-6},
author = {Qi Xia and Tielin Shi and Michael Yu Wang},
keywords = {Shape and topology optimization, Level set method, Eigenvalue, Vibration},
abstract = {We present a level set based shape and topology optimization method for maximizing the simple or repeated first eigenvalue of structure vibration. Considering that a simple eigenvalue is Fréchet differentiable but a repeated eigenvalue is only Gateaux differentiable, we take different approaches to derive the boundary variation. Boundary variation is treated as an advection velocity in the Hamilton–Jacobi equation.}
}

